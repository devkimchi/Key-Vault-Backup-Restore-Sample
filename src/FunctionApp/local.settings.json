{
  "IsEncrypted": false,
  "Values": {
    "AzureWebJobsStorage": "UseDevelopmentStorage=true",
    "FUNCTIONS_WORKER_RUNTIME": "dotnet",

    "KeyVault__Endpoints__Backup": "backup",
    "KeyVault__Endpoints__Restore": "restore",
    "KeyVault__Instances__Backup__BaseUri": "https://<KEYVAULT_NAME_FOR_BACKUP>.vault.azure.net/",
    "KeyVault__Instances__Restore__BaseUri": "https://<KEYVAULT_NAME_FOR_RESTORE>.vault.azure.net/",

    "Blob__Container__Name": "<BLOB_CONTAINER_NAME>"
  }
}
